{
  "applicationId": "com.example.memphisoptimizer",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/resources",
  "url": "/views/index.html",
  "enableServer": true,
  "logging": {
    "enabled": true,
    "writeToFile": true
  },
  "modes": {
    "window": {
      "title": "Memphis Optimizer v2",
      "width": 800,
      "height": 600,
      "minWidth": 400,
      "minHeight": 300,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "resourcesPath": "resources"
  }
}
